    "buffer": "^5.2.1",
    "css-element-queries": "^1.2.3",
    "file-saver": "~2.0.2",
    "localstorage-memory": "~1.0.3",
    "lodash-es": "~4.17.11",
    "ngx-toastr": "~13.2.1",
    "pdfjs-dist": "^2.6.347",
    "tslib": "^2.0.0",
    "xlsx": "^0.14.4",
    "jexl": "^2.3.0",
    "@types/file-saver": "^2.0.1",
    "webfontloader": "^1.6.28",
