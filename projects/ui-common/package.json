{
  "name": "@theseam/ui-common",
  "version": "0.1.18",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ../../dist/ui-common/",
    "copy:files": "cp --parents -p schematics/*/files/** ../../dist/ui-common/",
    "copy:collection": "cp schematics/collection.json ../../dist/ui-common/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
  },
  "peerDependencies": {
    "@angular/common": "^10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/forms": "^10.1.2",
    "@angular/animations": "^10.1.2",
    "@angular/router": "^10.1.2",
    "rxjs": "^6.5.5"
  },
  "dependencies": {
    "@angular/cdk": "^10.2.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@fortawesome/angular-fontawesome": "~0.5.0",
    "@fortawesome/fontawesome-svg-core": "~1.2.21",
    "@fortawesome/free-brands-svg-icons": "~5.10.1",
    "@fortawesome/free-regular-svg-icons": "~5.10.1",
    "@fortawesome/free-solid-svg-icons": "~5.10.1",
    "@marklb/file-type": "~10.6.5",
    "@marklb/ngx-datatable": "~18.0.0",
    "@marklb/ngx-loading": "^10.0.0",
    "@ng-select/ng-select": "~4.0.0",
    "@ng-bootstrap/ng-bootstrap": "~6.1.0",
    "overlayscrollbars": "~1.7.3",
    "@types/overlayscrollbars": "~1.7.0",
    "bootstrap": "~4.4.1",
    "buffer": "^5.2.1",
    "css-element-queries": "^1.2.3",
    "file-saver": "~2.0.2",
    "localstorage-memory": "~1.0.3",
    "lodash-es": "~4.17.11",
    "ngx-toastr": "~12.1.0",
    "pdfjs-dist": "2.0.943",
    "tslib": "^2.0.0",
    "xlsx": "^0.14.4",
    "jexl": "^2.3.0",
    "@types/file-saver": "^2.0.1",
    "webfontloader": "^1.6.28",
    "@types/webfontloader": "^1.6.29",
