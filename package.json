    "@storybook/addons": "5.2.0-beta.38",
    "@storybook/angular": "5.2.0-beta.38",
    "@storybook/source-loader": "5.2.0-beta.38",
    "@types/webpack-env": "^1.13.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/overlayscrollbars": "~1.7.0",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.0.0",
    "cpx": "^1.5.0",
    "eslint": "^5.10.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "just-wait": "^1.0.11",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "ts-node": "~7.0.0",
