    "xlsx": "~0.14.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular-devkit/build-ng-packagr": "~0.802.2",
    "@angular/cli": "~8.2.2",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@babel/core": "^7.5.5",
    "@storybook/addon-a11y": "5.2.0-beta.38",
    "@storybook/addon-actions": "5.2.0-beta.38",
