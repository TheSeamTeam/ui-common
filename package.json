{
  "name": "theseam-ui-common",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ui-common": "run-s build:ui-common:clean build:ui-common:ng-styles",
    "build:ui-common:clean": "rimraf dist/*",
    "build:ui-common:pre-styles": "run-s build:ui-common:wait-for-init build:ui-common:styles-assets",
    "build:ui-common:wait-for-init": "just-wait -p \"dist/ui-common\" -t 120",
    "build:ui-common:ng-styles": "run-p build:ui-common:ng build:ui-common:pre-styles",
    "build:ui-common:ng": "ng build ui-common",
    "build:ui-common:styles": "cpx -p projects/ui-common/src/styles/**/* dist/ui-common/styles",
    "build:ui-common:assets": "cpx -p projects/ui-common/src/assets/**/* dist/ui-common/assets",
    "build:ui-common:npmrc": "cpx -p .npmrc dist/ui-common",
    "build:ui-common:styles-assets": "run-p build:ui-common:styles build:ui-common:assets build:ui-common:npmrc",
    "build-w:ui-common": "run-s build-w:ui-common:clean build-w:ui-common:ng-styles",
    "build-w:ui-common:clean": "rimraf dist/*",
    "build-w:ui-common:pre-styles": "run-s build-w:ui-common:wait-for-init build-w:ui-common:styles-assets",
    "build-w:ui-common:wait-for-init": "just-wait -p \"dist/ui-common\" -t 120",
    "build-w:ui-common:ng-styles": "run-p build-w:ui-common:ng build-w:ui-common:pre-styles",
    "build-w:ui-common:ng": "ng build ui-common --watch",
    "build-w:ui-common:styles": "cpx -p projects/ui-common/src/styles/**/* dist/ui-common/styles --watch",
    "build-w:ui-common:assets": "cpx -p projects/ui-common/src/assets/**/* dist/ui-common/assets --watch",
    "build-w:ui-common:styles-assets": "run-p build-w:ui-common:styles build-w:ui-common:assets",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook -o dist/_docs",
    "refreshVSToken": "vsts-npm-auth -config .npmrc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/cdk": "~8.0.2",
    "@angular/common": "~8.2.0",
